<h1>Sign in</h1>
<% if flash[:danger] %>
  <p><%= flash[:danger] %></p>
<% end %>
<%= form_with url: session_path, local: true do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <%= f.submit "Sign In" %>
<% end %>
